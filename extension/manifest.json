{
  "manifest_version": 3,
  "name": "Twitter Blacklist",
  "version": "1.0",
  "host_permissions": [
    "https://twitter.com/*",
    "https://public-twitter-blacklist.s3.eu-central-1.amazonaws.com/*"
  ],
  "action": {
    "default_icon": {
      "128": "icons/clean.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"]
    }
  ]
}
